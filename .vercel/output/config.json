{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/orders/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/update-order-status/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/init-db/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/orders/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/favicon\\.ico/?$",
			"dest": "_render"
		},
		{
			"src": "^/manifest\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/products/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/services/?$",
			"dest": "_render"
		},
		{
			"src": "^/settings/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/thank-you/?$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}